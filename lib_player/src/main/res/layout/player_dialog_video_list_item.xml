<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:id="@+id/fl_video_list_item"
    android:layout_height="wrap_content"
    android:descendantFocusability="blocksDescendants"
    android:background="@drawable/player_shape_video_list_item"
    tools:background="@color/color_player_background">

    <TextView
        android:id="@+id/tv_video_list_item"
        android:layout_width="75dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="@android:color/transparent"
        android:gravity="center"
        android:padding="10dp"
        android:textColor="@color/player_selector_list_choice"
        android:textSize="14sp"
        tools:text="5" />

</FrameLayout>